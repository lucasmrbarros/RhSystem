@{
    ViewData["Title"] = "Vaga";
}
<div class="container-fluid">
    <div class="text-center">
        <h1 class="display-4">Cadastro de Vagas - Tecnologias</h1>
    </div>
</div>

<!-- Formulario que relaciona as vagas as tecnologias exigidas  -->
<div class="container p-5 my-5">
        <form form action="/" class="was-validated" id="postRelTecVaga">
            <div class="form-group">
                <label for="sel1">Selecione o a vaga:</label>
                <select class="form-control" id="Vaga-dropdown" name="vagaid">
                </select>
            </div >
            <div class="form-group">
                <label for="sel1">Selecione a tecnologia:</label>
                    <select class="form-control" id="dropdown_tecnologia" name="tecid">
                 </select>
            </div>
            <button type="submit" class="btn btn-primary" data-toggle="modal" data-target="#myModal">Enviar</button>
                <a class="btn btn-success" asp-area="" asp-controller="Home" asp-action="Candidatos">Continuar</a>
        </form>
</div>







